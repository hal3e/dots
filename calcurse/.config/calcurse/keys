generic-export  x
generic-goto  g
generic-other-cmd  ;
generic-config-menu  C
generic-redraw  ^R
generic-add-appt  ^A
generic-add-todo  ^T
generic-prev-day  T
generic-next-day  t
generic-prev-week  W
generic-next-week  w
generic-prev-month  M
generic-next-month  m
generic-prev-year  N
generic-next-year  n
generic-scroll-down  J
generic-scroll-up  K
generic-goto-today  ^G
generic-command  :
move-right  l
move-left  h
move-down  j
move-up  k
start-of-week  0
end-of-week  $
add-item  a
del-item  d
